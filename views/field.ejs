    <script type="text/javascript">
        $(document).ready(function () {
            // search field by id
            $("h3[id]").click(function () {
                var para = "content=" + $(this).attr("id") + "&group=field";
                search_now = $(this).attr("id");
                search_type = "field";
                $("#wait").show();
                $("#main_list").hide();
                $.ajax({
                    url: "action/query",
                    type: "post",
                    dataType: "json",
                    data: para,
                    success: showsearh
                });
            });

            $("h4[id]").click(function () {
                var para = "content=" + $(this).attr("id") + "&group=subfield";
                search_now = $(this).attr("id");
                search_type = "field";
                $("#wait").show();
                $("#main_list").hide();
                $.ajax({
                    url: "action/query",
                    type: "post",
                    dataType: "json",
                    data: para,
                    success: showsearh
                });
            });


        });
