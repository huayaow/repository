<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="combinatorial testing repository is a web based application to collect works that address the topic of combinatorial testing.">
    <meta name="keywords" content="repository, combinatorial testing, software testing, publication, research, paper">
    <meta name="author" content="Huayao">

    <title>Rank</title>
    <link rel="shortcut icon" href="image/favicon.ico" type="image/x-icon">
    <link rel="icon" href="image/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="http://gist.nju.edu.cn/css/bootstrap.min.css">
    <link rel="stylesheet" href="css/font-awesome.css">
    <link rel="stylesheet" href="css/repository.css">

    <script src="js/jquery.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#nav_content").html("Publication");
            $("#nav_publication").addClass("active");
            $("#c_rank").addClass("active");
        });
    </script>

</head>
<body>
<!-- Wrap all page content here -->
<div id="wrap">

    <!-- top navigation -->
    <%- include template_top.ejs %>

    <div class="container">
        <br/><br/>
        <br/><br/>

        <div class="row">
            <!-- list part -->
            <div class="col-md-2">
                <ul class="nav nav-pills nav-stacked">
                    <li><a href="./"><i class="fa fa-search"></i>&nbsp;&nbsp;Search</a></li>
                    <li><a href="statistic"><i class="fa fa-bars"></i>&nbsp;&nbsp;Statistic</a></li>
                    <li class="active"><a href="#"><i class="fa fa-star"></i>&nbsp;&nbsp;Rank (beta)</a></li>
                </ul>
                <br>

                <div class="panel panel-success">
                    <div class="panel-heading"><h4>Paper List</h4></div>
                    <div class="list-group">
                        <a href="all" class="list-group-item">All</a>
                        <a href="author" class="list-group-item">Author</a>
                        <a href="field" class="list-group-item">Research Field</a>
                        <a href="publication" class="list-group-item">Publication Venue</a>
                    </div>
                </div>
            </div>

            <!-- data part -->
            <div class="col-md-10">
                <h4>Top 30 Scholars</h4>
                <table id='atable' class='table listtable table-hover'>
                    <tr>
                        <th>#</th>
                        <th>Name</th>
                        <th>TSE</th>
                        <th>TOSEM</th>
                        <th>ESE</th>
                        <th>IST</th>
                        <th>JSS</th>
                        <th>STVR</th>
                        <th>FSE</th>
                        <th>ICSE</th>
                        <th>ASE</th>
                        <th>ISSRE</th>
                        <th>ISSTA</th>
                        <th>ICSM</th>
                        <th>Other</th>
                        <th>Sum</th>
                        <th>Score</th>
                    </tr>
                    <%
                        var i = 0 ;
                        ranking.forEach( function(each) {
                            i = i + 1 ;
                    %>
                            <tr>
                            <td><%=i%></td>
                            <td><%=each.author%></td>
                            <td><%=each.TSE%></td>
                            <td><%=each.TOSEM%></td>
                            <td><%=each.ESE%></td>
                            <td><%=each.IST%></td>
                            <td><%=each.JSS%></td>
                            <td><%=each.STVR%></td>
                            <td><%=each.FSE%></td>
                            <td><%=each.ICSE%></td>
                            <td><%=each.ASE%></td>
                            <td><%=each.ISSRE%></td>
                            <td><%=each.ISSTA%></td>
                            <td><%=each.ICSM%></td>
                            <td><%=each.Other%></td>
                            <td><%=each.Sum%></td>
                            <td><%=each.Score%></td>
                            </tr>
                    <%
                    });
                    %>
                </table>
            </div>
            <!-- span10 -->
        </div>
    </div>
    <!-- /container -->

    <div id="push"></div>
</div>
<!-- /wrap -->

<!-- footer -->
<%- include template_bottom.ejs %>

<script src="http://gist.nju.edu.cn/js/bootstrap.min.js"></script>
</body>
</html>